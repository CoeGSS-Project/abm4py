environment
    tabular_data

method
    sampling
        sample_type random
        samples = 5
        seed = 1

variables
    uniform_uncertain = 2
        descriptor      'innoPriority' 'mobIncomeShare'
        lower_bounds    0.0              0.1            
        upper_bounds    0.5              0.4            
    discrete_state_set 
        string = 2
            descriptors 'scenarioFileName'     'calcResultsScript'
            elements 'parameters_small.csv'    'rank.py'

interface
    fork                    
        analysis_drivers = '../fake-run-via-dakota.py'
        parameters_file = 'd-params.in'
        results_file = 'd-results.out'
        file_tag
        file_save

responses
    response_functions = 1
    descriptors 'rank' 
    no_gradients
    no_hessians

