environment
    tabular_data

method
    sampling
        sample_type random
        samples = 4
        seed = 1

variables
    uniform_uncertain = 2
        descriptor      'innoPriority' 'mobIncomeShare'
        lower_bounds    0.0              0.1            
        upper_bounds    0.5              0.4            
    discrete_state_set 
        string = 2
            descriptors 'scenarioFileName'     'calcResultsScript'
            elements 'parameters_med.csv'    'calc-error.py'

interface
    system asynchronous evaluation_concurrency = 2
        analysis_drivers = 'run-simulation.sh'
        parameters_file = 'd-params.in'
        results_file = 'd-results.out'
        file_tag
        file_save

responses
    response_functions = 1
    descriptors 'relativeError' # 'absoluteError'
    no_gradients
    no_hessians

