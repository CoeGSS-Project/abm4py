environment
    tabular_data

method
    sampling
        sample_type random
        samples = 3
        seed = 1

variables
    uniform_uncertain = 2
        lower_bounds    0.0              0.1            
        upper_bounds    0.5              0.4            
        descriptor      'innoPriority' 'mobIncomeShare'
    discrete_state_set 
        string = 1
            descriptors 'scenarioFileName'
            elements 'parameters_small.csv'

interface
    fork                                                        
        analysis_drivers = 'run-via-dakota.py'
        parameters_file = 'd-params.in'
        results_file = 'd-results.out'
        file_tag
        file_save

responses
    response_functions = 2
    descriptors 'relativeError' 'absoluteError'
    no_gradients
    no_hessians

